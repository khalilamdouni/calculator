<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:mvc="http://www.springframework.org/schema/mvc" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans     
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context 
        http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <bean class="org.springframework.orm.jpa.support.PersistenceAnnotationBeanPostProcessor"/>
    <bean class="org.springframework.dao.annotation.PersistenceExceptionTranslationPostProcessor"/>
    
    <bean id="jarManagerDao" class="org.calculator.dao.impl.JarManagerDao"></bean>
    <bean id="calculatorClassDao" class="org.calculator.dao.impl.CalculatorClassDao"></bean>
    <bean id="methodDao" class="org.calculator.dao.impl.MethodDao"></bean>
    <bean id="paramDao" class="org.calculator.dao.impl.ParamDao"></bean>
    <bean id="paramConfigDao" class="org.calculator.dao.impl.ParamConfigDao"></bean>
    <bean id="executionPlanDao" class="org.calculator.dao.impl.ExecutionPlanDao"></bean>
    <bean id="webScenarioDao" class="org.calculator.dao.impl.WebScenarioDao"></bean>
    <bean id="webRequestDao" class="org.calculator.dao.impl.WebRequestDao"></bean>
    <bean id="webParamDao" class="org.calculator.dao.impl.WebParamDao"></bean>

</beans>